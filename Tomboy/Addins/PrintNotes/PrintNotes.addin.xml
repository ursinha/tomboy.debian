<Addin id="PrintNotesAddin"
	namespace="Tomboy"
	name="Printing Support"
	author="Tomboy Project"
	description="Allows you to print a note."
	category="Desktop Integration"
	defaultEnabled="true"
	version="0.2.1">

	<Runtime>
		<Import assembly="PrintNotes.dll" />
	</Runtime>

	<Dependencies>
		<Addin id="Tomboy" version="0.10" />
	</Dependencies>

	<Extension path="/Tomboy/NoteAddins">
		<NoteAddin type="Tomboy.PrintNotes.PrintNotesNoteAddin" />
	</Extension>
</Addin>
